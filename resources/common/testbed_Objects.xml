<categories>
	<category name="cutlery"    defaultLocation="dining table" room="dining room">
		<object name="fork"     type="alike" difficulty="high" />
		<object name="knife"    type="alike" difficulty="high" />
		<object name="spoon"    type="alike" difficulty="high" />
	</category>

	<category name="drinks"         defaultLocation="counter" room="kitchen">
		<object name="bottle" type="alike" difficulty="easy" canPour="true" />
		<object name="milk" type="alike" difficulty="easy" canPour="true" />
	</category>

	<category name="tableware"  defaultLocation="counter" room="kitchen">
		<object name="bowl"     type="alike" difficulty="moderate" canPourIn="true"  />
		<object name="cup"    	type="alike" difficulty="moderate" canPourIn="true"  />
	</category>

</categories>

